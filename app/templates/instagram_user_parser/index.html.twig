    {# Display #}
    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Name</th>
                <th>Username</th>
                <th>Description</th>
            </tr>
        </thead>
        <tbody>
        {% for user in users %}
            <div>
                {% for visual in user.visuals %}
                <div>
                    <img src="{{ visual.path }}">
